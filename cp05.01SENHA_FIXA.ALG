algoritmo "senha_fixa"

var
   senha, x : inteiro

inicio
   x <- 2002

   escreva("Digite a senha: ")
   leia(senha)
   
   enquanto senha <> x faca
      escreva("Senha Invalida! Tente novamente: ")
      leia(senha)
      
      se senha = x entao
         escreval("Acesso permitido!")
      fimse
      
   fimenquanto
   


fimalgoritmo